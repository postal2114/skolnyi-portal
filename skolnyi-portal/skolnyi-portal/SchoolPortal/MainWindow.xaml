<Window x:Class="SchoolPortal.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SchoolPortal"
        mc:Ignorable="d"
        Title="MainWindow" Height="1024" Width="1024">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="59*"/>
            <RowDefinition Height="949*"/>
        </Grid.RowDefinitions>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="frame_ContentRendered" Grid.Row="1" Name="frame" Margin="0,18,0,0" ></Frame>
        <Button Name="Back" Click="Button_Click" HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="18" Width="100" Height="28">Назад</Button>
    </Grid>
</Window>
